{
    "name": "nirou8/php-multiple-saml",
    "description": "OneLogin PHP SAML For Multiple SPs Toolkit",
    "license": "MIT",
    "homepage": "https://github.com/nirou8/php-multiple-saml",
    "keywords": ["saml", "saml2", "onelogin"],
    "autoload": {
        "classmap": [
            "extlib/xmlseclibs",
            "lib/Saml",
            "lib/Saml2"
        ]
    },
    "version": "1.0.0",
    "authors": [
        {
            "name": "Nadhir",
            "email": "khayati.nadhir@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-dom": "*",
        "ext-mcrypt": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8",
        "satooshi/php-coveralls": "1.0.1",
        "sebastian/phpcpd": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend" : "1.1.0",
        "squizlabs/php_codesniffer": "2.9.0"
